<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/background_end"
    tools:context=".BullsAndCowsActivity_end">

    <TextView
        android:id="@+id/textView2"
        android:layout_width="279dp"
        android:layout_height="88dp"
        android:layout_marginBottom="40dp"
        android:text="@string/End"
        android:textAlignment="center"
        android:textAppearance="@style/TextAppearance.AppCompat.Medium"
        android:textColor="#FFFFFF"
        android:textSize="20sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toTopOf="@+id/button_to_main"
        app:layout_constraintEnd_toEndOf="@+id/button_to_main"
        app:layout_constraintStart_toStartOf="@+id/button_to_main"
        tools:textAlignment="center" />

    <Button
        android:id="@+id/button_to_main"
        android:layout_width="182dp"
        android:layout_height="39dp"
        android:text="@string/Button_to_main"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />
</androidx.constraintlayout.widget.ConstraintLayout>